<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <doofinder_feed_feed>
            <feed_attributes>
                <id>df_id</id>
                <title>name</title>
                <description>short_description</description>
                <brand>manufacturer</brand>
                <link>url_key</link>
                <image_link>image_link</image_link>
                <price>price</price>
                <mpn>sku</mpn>
                <availability>availability</availability>
                <currency>currency</currency>
            </feed_attributes>
            <feed_settings>
                <export_product_prices>1</export_product_prices>
                <image_size></image_size>
                <split_grouped_products>0</split_grouped_products>
            </feed_settings>
            <feed_cron>
                <enabled>1</enabled>
                <start_time>0,0,0</start_time>
                <frequency>D</frequency>
                <step_size>1000</step_size>
                <step_delay>5</step_delay>
            </feed_cron>
        </doofinder_feed_feed>

        <directives>
            <df_id><![CDATA[Product Id]]></df_id>
        </directives>
    </default>
</config>
