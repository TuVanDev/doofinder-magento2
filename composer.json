{
    "name": "doofinder/doofinder-magento2",
    "description": "Doofinder module for Magento 2",
    "require": {
        "php": "~5.5.0|~5.6.0|~7.0.0",
        "magento/magento-composer-installer": "*",
        "sabre/xml": "^1.4"
    },
    "type": "magento2-module",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "autoload": {
        "files": [
            "registration.php",
            "lib/Doofinder/doofinder_management_api.php"
        ],
        "psr-4": {
            "Doofinder\\Feed\\": "",
            "Magento\\Framework\\": "/var/www/html/lib/internal/Magento/Framework/",
            "Magento\\Setup\\": "/var/www/html/setup/src/Magento/Setup/"
        }
    }
}
